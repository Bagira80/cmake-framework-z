<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--
    This property file is automatically generated by 'luchs' for CMake project
    '@PROJECT_NAME@'. Do not edit it directly!
    Instead feel free to create and edit a file '$(MSBuildProjectName).targets'
    in directory '@PROJECT_SOURCE_DIR@\luchs\'.
    (It will automatically be imported if found.)
  -->


  <!-- 1. Re-introduce the mechanism that was replaced in the   -->
  <!--    associated *.vcxproj file in order to load this file. -->
  <Import Project="$(UserRootDir)\Microsoft.Cpp.$(Platform).user.props"
          Condition="exists('$(UserRootDir)\Microsoft.Cpp.$(Platform).user.props')"
          Label="LocalAppDataPlatform" />

  <!-- 2. Import the (common) global *.props file if available. -->
  <Import Project="@CMAKE_BINARY_DIR@\luchs.MSBuild.Props\GlobalLuchsSettings.props"
          Condition="exists('@CMAKE_BINARY_DIR@\luchs.MSBuild.Props\GlobalLuchsSettings.props')" />

  <!-- 3. Import the (custom) associated *.targets file if available. -->
  <Import Project="@PROJECT_SOURCE_DIR@\luchs\$(MSBuildProjectName).targets"
          Condition="exists('@PROJECT_SOURCE_DIR@\luchs\$(MSBuildProjectName).targets')" />
